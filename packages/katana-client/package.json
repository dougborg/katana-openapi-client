{
  "name": "katana-openapi-client",
  "version": "0.0.1",
  "description": "TypeScript client for Katana Manufacturing ERP API with automatic retries, rate limiting, and pagination",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "generate": "openapi-ts",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "katana",
    "manufacturing",
    "erp",
    "api",
    "client",
    "openapi",
    "typescript"
  ],
  "author": "Doug Borg",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dougborg/katana-openapi-client.git",
    "directory": "packages/katana-client"
  },
  "bugs": {
    "url": "https://github.com/dougborg/katana-openapi-client/issues"
  },
  "homepage": "https://github.com/dougborg/katana-openapi-client/tree/main/packages/katana-client#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@hey-api/client-fetch": "^0.13.1",
    "@hey-api/openapi-ts": "^0.88.0",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.16.0",
    "msw": "^2.6.8",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "sideEffects": false
}
